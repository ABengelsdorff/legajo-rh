{
  "name": "legajo",
  "version": "1.0.1",
  "description": "Aplicación de escritorio para gestión de legajos",
  "main": "dist-electron/main.js",
  "author": "Bengelsdorff Angélica",
  "license": "MIT",
  "scripts": {
  "clean": "rimraf dist dist-electron electron/backend-dist front/out back/dist",
  "copy-backend": "xcopy /E /I /Y back\\dist electron\\backend-dist && xcopy /E /I /Y back\\node_modules electron\\backend-dist\\node_modules",
  "build:front": "npm --prefix front run build",
  "build:back": "npm --prefix back run build",
  "build:electron": "tsc -p tsconfig.electron.json",
  "rebuild": "electron-rebuild -f -w sqlite3",
  "start:electron": "electron dist-electron/main.js",
  "dev": "npm run build:electron && npm run start:electron",
  "build": "npm run clean && npm run build:front && npm run build:back && npm run build:electron && npm run copy-backend && npm run rebuild && electron-builder"
},
  "build": {
    "appId": "com.legajo.app",
    "productName": "Legajo",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "dist-electron/**/*",
      "electron/backend-dist/**/*",
      "front/out/**/*",
      "database.sqlite",
      {
        "from": "back/node_modules",
        "to": "electron/backend-dist/node_modules"
      }
    ],
    "extraMetadata": {
      "main": "dist-electron/main.js"
    },
    "asar": false,
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
    "oneClick": false,                           
    "perMachine": false,                        
    "allowToChangeInstallationDirectory": true,  
    "createDesktopShortcut": true,               
    "createStartMenuShortcut": true,             
    "shortcutName": "Legajo"                     
  }
  },
  "dependencies": {
    "boolean": "^3.2.0",
    "buffer-crc32": "^0.2.13",
    "cacheable-lookup": "^5.0.4",
    "cacheable-request": "^7.0.4",
    "clone-response": "^1.0.3",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "decompress-response": "^6.0.0",
    "defer-to-connect": "^2.0.1",
    "define-data-property": "^1.1.4",
    "define-properties": "^1.2.1",
    "detect-node": "^2.1.0",
    "end-of-stream": "^1.4.4",
    "env-paths": "^2.2.1",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es6-error": "^4.1.1",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.21.2",
    "extract-zip": "^2.0.1",
    "fd-slicer": "^1.1.0",
    "fs-extra": "^8.1.0",
    "get-stream": "^5.2.0",
    "global-agent": "^3.0.0",
    "globalthis": "^1.0.4",
    "gopd": "^1.2.0",
    "got": "^11.8.6",
    "graceful-fs": "^4.2.11",
    "has-property-descriptors": "^1.0.2",
    "http-cache-semantics": "^4.1.1",
    "http2-wrapper": "^1.0.3",
    "json-buffer": "^3.0.1",
    "json-stringify-safe": "^5.0.1",
    "jsonfile": "^4.0.0",
    "keyv": "^4.5.4",
    "lowercase-keys": "^2.0.0",
    "matcher": "^3.0.0",
    "mimic-response": "^1.0.1",
    "ms": "^2.1.3",
    "normalize-url": "^6.1.0",
    "object-keys": "^1.1.1",
    "once": "^1.4.0",
    "p-cancelable": "^2.1.1",
    "pend": "^1.2.0",
    "progress": "^2.0.3",
    "pump": "^3.0.2",
    "quick-lru": "^5.1.1",
    "resolve-alpn": "^1.2.1",
    "responselike": "^2.0.1",
    "roarr": "^2.15.4",
    "semver": "^6.3.1",
    "semver-compare": "^1.0.0",
    "serialize-error": "^7.0.1",
    "sprintf-js": "^1.1.3",
    "sumchecker": "^3.0.1",
    "type-fest": "^0.13.1",
    "undici-types": "^6.20.0",
    "universalify": "^0.1.2",
    "wrappy": "^1.0.2",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.3"
  }
}
